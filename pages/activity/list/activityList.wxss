.topBtnGroup {
  width: 100%;
  height: 120rpx;
  position: fixed;
  top: 0;
  background-color: white;
  /* box-shadow: 0rpx -10rpx 25rpx #ccc; */
  display: flex;
  align-items: center;
}
.topBtnGroup .item {
  width: 375rpx;
  height: 40rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}
.topBtnGroup .item view {
  font-size: 26rpx;
  color: #515151;
}
.topBtnGroup .item image {
  width: 26rpx;
  height: 26rpx;
  margin-left: 3rpx;
}